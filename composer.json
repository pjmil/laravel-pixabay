{
    "name": "pjmil/laravel-pixabay",
    "description": "Laravel facade for accessing the Pixabay API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Millar",
            "email": "pjmil116@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "laravel/framework": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": { 
            "Pjmil\\Pixabay\\": "src/" 
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Pjmil\\Pixabay\\tests\\": "tests/" 
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}